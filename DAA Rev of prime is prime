#include <stdio.h>
int isPrime(int num) {
    if (num < 2) {
        return 0;
    }
    
    for (int i = 2; i * i <= num; i++) {
        if (num % i == 0) {
            return 0;
        }
    }
    
    return 1;
}
int reverseNumber(int num) {
    int rev = 0;
    while (num != 0) {
        rev = rev * 10 + num % 10;
        num /= 10;
    }
    
    return rev;
}

int main() {
    int n;
    
    printf("Enter the limit: ");
    scanf("%d", &n);
    
    printf("Prime numbers where the reverse is also prime:\n");
    for (int i = 2; i <= n; i++) {
        if (isPrime(i) && isPrime(reverseNumber(i))) {
            printf("%d\n", i);
        }
    }
    
    return 0;
}
